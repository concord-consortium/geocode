    "eslint-webpack-plugin": "^5.0.1",
    "fs-extra": "^11.3.0",
    "ftp": "^0.3.10",
    "globals": "^16.0.0",
    "html-webpack-plugin": "^5.6.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
