{
  "name": "geocode",
  "version": "2.9.1",
  "description": "Concord Consortium GeoCode project",
  "main": "index.js",
  "jest": {
    "testEnvironment": "jsdom",
    "testEnvironmentOptions": {
      "url": "https://geocode-projects.nonexistent.url.com"
    },
    "setupFilesAfterEnv": [
      "<rootDir>src/setupTests.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/cypress/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "src/utilities/test-utils.ts"
    ],
    "moduleNameMapper": {
      "\\.svg$": "<rootDir>/__mocks__/svgMock.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|sass)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "scripts": {
    "start": "webpack serve",
    "build": "npm-run-all lint:build clean build:webpack",
    "build:compile": "tsc --project .",
    "build:top-test": "cross-env DEPLOY_PATH=specific/release/ webpack --mode production --output-path top-test/specific/release && cp top-test/specific/release/index-top.html top-test/",
    "build:webpack": "webpack --mode production",
    "clean": "rimraf dist",
    "lint": "eslint",
    "lint:build": "eslint --config eslint.build.config.mjs",
    "lint:fix": "eslint --fix",
    "lint:style": "eslint . --ext .js,.jsx,.ts,.tsx --config eslint.style.config.mjs",
    "lint:style:fix": "eslint . --ext .js,.jsx,.ts,.tsx --config eslint.style.config.mjs --fix",
    "lint:unused": "tsc --noUnusedLocals --project .",
    "fetch-data:velocity": "node src/assets/data/seismic/scripts/fetch-velocity-data.js",
    "fetch-data:position": "node src/assets/data/seismic/scripts/fetch-position-data.js",
    "test": "jest",
    "test:all": "npm-run-all build test start",
    "test:coverage": "jest --coverage",
    "test:debug": "node --nolazy --inspect-brk ./node_modules/.bin/jest --runInBand --no-cache",
    "test:watch": "jest --watchAll",
    "test:coverage:watch": "jest --coverage --watchAll",
    "test:full": "npm-run-all test test:cypress",
    "test:cypress": "cypress run --config video=false,defaultCommandTimeout=15000 --env testEnv=local",
    "test:cypress:ci": "cypress run --config video=false,defaultCommandTimeout=15000 --env testEnv=local --record",
    "test:cypress:smoke": "cypress run --spec 'cypress/integration/smoke/*' --config video=false,defaultCommandTimeout=15000 --env testEnv=local",
    "test:cypress:branch": "cypress run --spec 'cypress/integration/branch/**/*' --config video=false,defaultCommandTimeout=15000 --env testEnv=local --record",
    "test:production": "cypress open --env testEnv=production",
    "test:master": "cypress open --env testEnv=master ",
    "test:branch": "cypress open --env testEnv=branch",
    "test:local": "cypress open --env testEnv=local",
    "test:cypress:open": "cypress open --env testEnv=local"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/concord-consortium/geocode.git"
  },
  "author": "Concord Consortium",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/concord-consortium/geocode/issues"
  },
  "homepage": "https://github.com/concord-consortium/geocode#readme",
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^6.0.4",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
    "@eslint/js": "^9.25.1",
    "@simonsmith/cypress-image-snapshot": "^9.1.0",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@stylistic/eslint-plugin-js": "^4.2.0",
    "@stylistic/eslint-plugin-jsx": "^4.2.0",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^12.1.5",
    "@types/color": "^4.2.0",
    "@types/d3": "^7.4.3",
    "@types/delaunator": "^5.0.3",
    "@types/geojson": "^7946.0.16",
    "@types/jest": "^29.5.14",
    "@types/js-beautify": "^1.14.3",
    "@types/leaflet": "^1.9.17",
