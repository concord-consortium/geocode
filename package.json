    "script-loader": "^0.7.2",
    "style-loader": "^3.3.3",
    "ts-jest": "^26.5.6",
    "ts-loader": "^5.3.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.20.1",
    "tslint-loader": "^3.6.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.9.10",
    "url-loader": "^1.1.2",
    "wait-on": "^7.0.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.10.1"
  },
  "dependencies": {
    "@inlet/react-pixi": "^0.5.7",
    "@material-ui/core": "^4.12.3",
    "@svgr/webpack": "^4.3.3",
    "@types/d3": "^5.16.4",
    "@types/d3-contour": "^1.3.3",
    "@types/delaunator": "^3.0.0",
    "@types/mathjs": "^6.0.12",
    "@types/react-faux-dom": "^4.1.5",
    "@types/react-leaflet": "^2.8.2",
    "@types/recharts": "^1.8.23",
    "axios": "^0.21.4",
    "chai-string": "^1.5.0",
    "color": "^3.2.1",
    "d3": "^5.16.0",
    "d3-contour": "^1.3.2",
    "deepmerge": "^4.2.2",
    "delaunator": "^4.0.1",
    "iframe-phone": "^1.3.1",
    "immutable": "^3.8.2",
    "js-beautify": "^1.14.0",
    "js-interpreter": "^1.4.6",
    "leaflet": "^1.7.1",
    "leaflet-kmz": "^0.3.8",
    "less-loader": "^5.0.0",
    "lodash": "^4.17.21",
    "mathjs": "^7.6.0",
    "mobx": "^5.15.7",
    "mobx-react": "^5.4.3",
    "mobx-state-tree": "^3.17.3",
    "node-polyfill-webpack-plugin": "^4.0.0",
    "pixi.js": "^4.8.9",
