{
  "name": "geocode",
  "version": "2.8.0",
  "description": "Concord Consortium GeoCode project",
  "main": "index.js",
  "jest": {
    "testURL": "https://geocode-projects.unexisting.url.com",
    "setupFilesAfterEnv": [
      "<rootDir>src/setupTests.js"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/cypress/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "src/utilities/test-utils.ts"
    ],
    "moduleNameMapper": {
      "\\.svg$": "<rootDir>/__mocks__/svgMock.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|sass)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "scripts": {
    "start": "webpack serve",
    "build": "npm-run-all lint:build clean build:webpack",
    "build:top-test": "cross-env DEPLOY_PATH=specific/release/ webpack --mode production --output-path top-test/specific/release && cp top-test/specific/release/index-top.html top-test/",
    "build:webpack": "webpack --mode production --devtool false",
    "clean": "rimraf dist",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "lint:build": "tslint -c tslint-build.json 'src/**/*.{ts,tsx}'",
    "lint:fix": "tslint -c tslint-build.json --fix 'src/**/*.{ts,tsx}'",
    "lint:unused": "tsc --noUnusedLocals --project .",
    "fetch-data:velocity": "node src/assets/data/seismic/scripts/fetch-velocity-data.js",
    "fetch-data:position": "node src/assets/data/seismic/scripts/fetch-position-data.js",
    "test": "jest",
    "test:all": "npm-run-all build test start",
    "test:coverage": "jest --coverage",
    "test:debug": "node --nolazy --inspect-brk ./node_modules/.bin/jest --runInBand --no-cache",
    "test:watch": "jest --watchAll",
    "test:coverage:watch": "jest --coverage --watchAll",
    "test:full": "npm-run-all test test:cypress",
    "test:cypress": "cypress run --config video=false,defaultCommandTimeout=15000 --env testEnv=local",
    "test:cypress:ci": "cypress run --config video=false,defaultCommandTimeout=15000 --env testEnv=local --record",
    "test:cypress:smoke": "cypress run --spec 'cypress/integration/smoke/*' --config video=false,defaultCommandTimeout=15000 --env testEnv=local",
    "test:cypress:branch": "cypress run --spec 'cypress/integration/branch/**/*' --config video=false,defaultCommandTimeout=15000 --env testEnv=local --record",
    "test:production": "cypress open --env testEnv=production",
    "test:master": "cypress open --env testEnv=master ",
    "test:branch": "cypress open --env testEnv=branch",
    "test:local": "cypress open --env testEnv=local",
    "test:cypress:open": "cypress open --env testEnv=local"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/concord-consortium/geocode.git"
  },
  "author": "Concord Consortium",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/concord-consortium/geocode/issues"
  },
  "homepage": "https://github.com/concord-consortium/geocode#readme",
  "devDependencies": {
    "@concord-consortium/lara-interactive-api": "^1.7.0",
    "@cypress/webpack-preprocessor": "^4.1.5",
    "@types/color": "^3.0.3",
    "@types/enzyme": "^3.10.11",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^23.3.10",
    "@types/js-beautify": "^1.13.3",
    "@types/lodash": "^4.14.179",
    "@types/pixi.js": "^4.8.6",
    "@types/query-string": "^6.3.0",
    "@types/react": "^16.14.23",
    "@types/react-dom": "^16.9.14",
    "@types/react-syntax-highlighter": "^10.2.1",
    "@types/react-tabs": "^2.3.4",
    "@types/semver": "^7.3.12",
    "@types/styled-components": "^5.1.24",
    "@types/uuid": "^3.4.10",
    "autoprefixer": "^9.8.8",
    "babel-core": "^6.26.3",
    "babel-jest": "^24.7.1",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^3.4.0",
    "cypress": "^9.5.1",
    "cypress-commands": "^1.1.0",
    "cypress-image-snapshot": "^3.1.1",
    "dsv-loader": "^2.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "file-loader": "^2.0.0",
    "ftp": "^0.3.10",
    "html-webpack-plugin": "^5.5.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.7.1",
    "mini-css-extract-plugin": "^0.4.5",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "process": "^0.11.10",
    "rimraf": "^2.6.2",
